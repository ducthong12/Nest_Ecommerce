{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json",
    "assets": []
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json",
        "assets": [
          {
            "include": "../../../common/proto/user.proto",
            "outDir": "./dist/apps/api-gateway/user.proto",
            "watchAssets": true
          },
          {
            "include": "../../../common/proto/product.proto",
            "outDir": "./dist/apps/api-gateway/product.proto",
            "watchAssets": true
          }
        ]
      }
    },
    "analytics": {
      "type": "application",
      "root": "apps/analytics",
      "entryFile": "main",
      "sourceRoot": "apps/analytics/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analytics/tsconfig.app.json"
      }
    },
    "storage": {
      "type": "application",
      "root": "apps/storage",
      "entryFile": "main",
      "sourceRoot": "apps/storage/src",
      "compilerOptions": {
        "tsConfigPath": "apps/storage/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json",
        "assets": [
          {
            "include": "../../../common/proto/user.proto",
            "outDir": "./dist/apps/user/user.proto",
            "watchAssets": true
          }
        ]
      }
    },
    "inventory": {
      "type": "application",
      "root": "apps/inventory",
      "entryFile": "main",
      "sourceRoot": "apps/inventory/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inventory/tsconfig.app.json",
        "assets": [
          {
            "include": "../../../common/proto/inventory.proto",
            "outDir": "./dist/apps/inventory/inventory.proto",
            "watchAssets": true
          }
        ]
      }
    },
    "order": {
      "type": "application",
      "root": "apps/order",
      "entryFile": "main",
      "sourceRoot": "apps/order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order/tsconfig.app.json",
        "assets": [
          {
            "include": "../../../common/proto/order.proto",
            "outDir": "./dist/apps/order/order.proto",
            "watchAssets": true
          }
        ]
      }
    },
    "product": {
      "type": "application",
      "root": "apps/product",
      "entryFile": "main",
      "sourceRoot": "apps/product/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product/tsconfig.app.json",
        "assets": [
          {
            "include": "../../../common/proto/product.proto",
            "outDir": "./dist/apps/product/product.proto",
            "watchAssets": true
          }
        ]
      }
    },
    "mongodb": {
      "type": "library",
      "root": "libs/mongodb",
      "entryFile": "index",
      "sourceRoot": "libs/mongodb/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mongodb/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    }
  }
}