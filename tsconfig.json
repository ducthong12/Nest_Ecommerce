{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "yes/database": [
        "libs/database/src"
      ],
      "yes/database/*": [
        "libs/database/src/*"
      ],
      "yes/mongodb": [
        "libs/mongodb/src"
      ],
      "yes/mongodb/*": [
        "libs/mongodb/src/*"
      ],
      "yes/constants": [
        "libs/constants/src"
      ],
      "yes/constants/*": [
        "libs/constants/src/*"
      ],
      "yes/decorators": [
        "libs/decorators/src"
      ],
      "yes/decorators/*": [
        "libs/decorators/src/*"
      ],
      "@common/constants/*": [
        "common/constants/*"
      ],
      "@common/decorators/*": [
        "common/decorators/*"
      ],
      "@common/guards/*": [
        "common/guards/*"
      ],
      "@app/redis": [
        "libs/redis/src"
      ],
      "@app/redis/*": [
        "libs/redis/src/*"
      ],
      "@app/bullmq": [
        "libs/bullmq/src"
      ],
      "@app/bullmq/*": [
        "libs/bullmq/src/*"
      ]
    }
  }
}